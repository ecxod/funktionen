{
    "name": "ecxod/funktionen",
    "description": "Langweilige Funktionen die man immer braucht",
    "type": "library",
    "keywords": ["ecxod", "functions", "funktionen"],
    "license": "MIT",
    "autoload": {
        "files": [
            "src/funktionen/F.php",
            "tests/FTest.php"
        ],
        "psr-4": {
            "Ecxod\\Funtionen\\": "src/funktionen/",
            "Ecxod\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Christian Eichert",
            "email": "c@zp1.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2|^7.4|^8.0|^8.2",
        "sentry/sentry": "^4.8",
        "vlucas/phpdotenv": "^5.6",
        "erusev/parsedown": "^1.7"
    },
    "suggests": {
        "phpunit/phpunit": "^11.5"
    },
    "archive": {
        "exclude": [
            ".github",
            ".vscode",
            "tests",
            "vendor"
        ]
    }
}
